{
	"name": "sync-rules",
	"version": "1.0.0",
	"description": "sync-rules",
	"type": "module",
	"scripts": {
	  "sync": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-specifier-resolution=node\" tsx Chores/engineering/sync/main.ts",
